CAAC,CAAC;IACf","file":"D:/project/test/src/components/Register.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <div class=\"register-container\">\r\n        <el-card class=\"box-card\">\r\n            <div slot=\"header\" class=\"clearfix\">\r\n                <el-radio-group v-model=\"selectedOption\" class=\"radio-group\" @change=\"handleRadioChange\">\r\n                    <el-radio-button label=\"login\">登录</el-radio-button>\r\n                    <el-radio-button label=\"register\">注册</el-radio-button>\r\n                </el-radio-group>\r\n            </div>\r\n            <el-form @submit.prevent=\"handleRegister\">\r\n                <el-form-item label=\"邮箱\">\r\n                    <el-input type=\"email\" v-model=\"email\" required></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"密码\">\r\n                    <el-input :type=\"passwordType\" v-model=\"password\" required>\r\n                        <template #suffix>\r\n                            <el-button icon=\"el-icon-view\" @click=\"togglePasswordVisibility\" circle></el-button>\r\n                        </template>\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" native-type=\"submit\" class=\"register-button\">注册</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        data() {\r\n            return {\r\n                selectedOption: 'register',\r\n                email: '',\r\n                password: '',\r\n                passwordType: 'password'\r\n            };\r\n        },\r\n        methods: {\r\n            handleRadioChange(value) {\r\n                if (value === 'login') {\r\n                    this.$router.push('/login');\r\n                } else if (value === 'register') {\r\n                    this.$router.push('/register');\r\n                }\r\n            },\r\n            handleRegister() {\r\n                const users = JSON.parse(localStorage.getItem('users')) || [];\r\n                const userExists = users.some(u => u.email === this.email);\r\n                if (userExists) {\r\n                    alert('该邮箱已注册');\r\n                } else {\r\n                    users.push({ email: this.email, password: this.password });\r\n                    localStorage.setItem('users', JSON.stringify(users));\r\n                    alert('注册成功');\r\n                    // 这里可以进行跳转到登录页面\r\n                }\r\n            },\r\n            togglePasswordVisibility() {\r\n                this.passwordType = this.passwordType === 'password' ? 'text' : 'password';\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    .register-container {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        height: 100vh;\r\n    }\r\n\r\n    .box-card {\r\n        width: 400px;\r\n    }\r\n\r\n    .radio-group {\r\n        display: flex;\r\n        justify-content: center;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .register-button {\r\n        width: 100%;\r\n    }\r\n</style>\r\n"]}]}