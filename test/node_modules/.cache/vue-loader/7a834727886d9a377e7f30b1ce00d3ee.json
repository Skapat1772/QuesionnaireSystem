       <el-input :type=\"passwordType\" v-model=\"password\" required>\r\n                        <template #suffix>\r\n                            <el-button icon=\"el-icon-view\" @click=\"togglePasswordVisibility\" circle></el-button>\r\n                        </template>\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" native-type=\"submit\" class=\"login-button\">登录</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        data() {\r\n            return {\r\n                selectedOption: 'login',\r\n                identifier: '',\r\n                password: '',\r\n                passwordType: 'password'\r\n            };\r\n        },\r\n        methods: {\r\n            handleRadioChange(value) {\r\n                if (value === 'login') {\r\n                    this.$router.push('/login');\r\n                } else if (value === 'register') {\r\n                    this.$router.push('/register');\r\n                } else if (value === 'forgotPassword') {\r\n                    this.$router.push('/forgot-password');\r\n                }\r\n            },\r\n            handleLogin() {\r\n                const users = JSON.parse(localStorage.getItem('users')) || [];\r\n                const user = users.find(u => (u.email === this.identifier || u.username === this.identifier) && u.password === this.password);\r\n                if (user) {\r\n                    alert('登录成功');\r\n                    localStorage.setItem('currentUser', JSON.stringify(user)); // 存储当前用户信息\r\n                    this.$router.push({ name: 'Main' });\r\n                } else {\r\n                    alert('登录失败，请检查用户名或邮箱和密码');\r\n                }\r\n            },\r\n            togglePasswordVisibility() {\r\n                this.passwordType = this.passwordType === 'password' ? 'text' : 'password';\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    .login-container {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        height: 100vh;\r\n    }\r\n\r\n    .box-card {\r\n        width: 400px;\r\n    }\r\n\r\n    .radio-group {\r\n        display: flex;\r\n        justify-content: center;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .login-button {\r\n        width: 100%;\r\n    }\r\n</style>\r\n"]}]}