CAAC,CAAC;IACf","file":"D:/project/test/src/components/Register.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <div class=\"register-container\">\r\n        <el-card class=\"box-card\">\r\n            <div slot=\"header\" class=\"clearfix\">\r\n                <el-radio-group v-model=\"selectedOption\" class=\"radio-group\" @change=\"handleRadioChange\">\r\n                    <el-radio-button label=\"login\">登录</el-radio-button>\r\n                    <el-radio-button label=\"register\">注册</el-radio-button>\r\n                </el-radio-group>\r\n            </div>\r\n            <el-form @submit.prevent=\"handleRegister\">\r\n                <el-form-item label=\"邮箱\">\r\n                    <el-input type=\"email\" v-model=\"email\" required></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"用户名\">\r\n                    <el-input v-model=\"username\" required></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"密码\">\r\n                    <el-input :type=\"passwordType\" v-model=\"password\" required>\r\n                        <template #suffix>\r\n                            <el-button icon=\"el-icon-view\" @click=\"togglePasswordVisibility\" circle></el-button>\r\n                        </template>\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"密保问题\">\r\n                    <el-select v-model=\"securityQuestion\" placeholder=\"请选择密保问题\" required>\r\n                        <el-option label=\"你的名字是？\" value=\"你的名字是？\"></el-option>\r\n                        <el-option label=\"你的生日是？\" value=\"你的生日是？\"></el-option>\r\n                        <el-option label=\"你最喜欢的水果是？\" value=\"你最喜欢的水果是？\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"密保答案\">\r\n                    <el-input v-model=\"securityAnswer\" required></el-input>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" native-type=\"submit\" class=\"register-button\">注册</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from 'axios';\r\n\r\n    export default {\r\n        data() {\r\n            return {\r\n                selectedOption: 'register',\r\n                email: '',\r\n                username: '',\r\n                password: '',\r\n                passwordType: 'password',\r\n                securityQuestion: '',\r\n                securityAnswer: ''\r\n            };\r\n        },\r\n        methods: {\r\n            handleRadioChange(value) {\r\n                if (value === 'login') {\r\n                    this.$router.push('/login');\r\n                } else if (value === 'register') {\r\n                    this.$router.push('/register');\r\n                }\r\n            },\r\n            async handleRegister() {\r\n                try {\r\n                    // 检查邮箱是否重复\r\n                    const emailCheckResponse = await axios.post('http://localhost:3000/check-email', {\r\n                        email: this.email\r\n                    });\r\n                    if (emailCheckResponse.data.exists) {\r\n                        alert('邮箱已使用');\r\n                        return;\r\n                    }\r\n\r\n                    // 检查用户名是否重复\r\n                    const usernameCheckResponse = await axios.post('http://localhost:3000/check-username', {\r\n                        username: this.username\r\n                    });\r\n                    if (usernameCheckResponse.data.exists) {\r\n                        alert('用户名已使用');\r\n                        return;\r\n                    }\r\n\r\n                    // 注册用户\r\n                    const registerResponse = await axios.post('http://localhost:3000/register', {\r\n                        email: this.email,\r\n                        username: this.username,\r\n                        password: this.password,\r\n                        question: this.securityQuestion,\r\n                        answer: this.securityAnswer\r\n                    });\r\n                    if (registerResponse.data.success) {\r\n                        alert('注册成功');\r\n                        this.$router.push('/login');\r\n                    } else {\r\n                        alert('用户名或邮箱已使用');\r\n                    }\r\n                } catch (error) {\r\n                    console.error(error);\r\n                    alert('用户名或邮箱已使用');\r\n                }\r\n            },\r\n            togglePasswordVisibility() {\r\n                this.passwordType = this.passwordType === 'password' ? 'text' : 'password';\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    .register-container {\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        height: 100vh;\r\n    }\r\n\r\n    .box-card {\r\n        width: 400px;\r\n    }\r\n\r\n    .radio-group {\r\n        display: flex;\r\n        justify-content: center;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .register-button {\r\n        width: 100%;\r\n    }\r\n</style>\r\n"]}]}